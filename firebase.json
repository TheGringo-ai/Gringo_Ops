{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/fredfix/**",
        "destination": "/fredfix/index.html"
      },
      {
        "source": "/launcher/**",
        "destination": "/launcher/index.html"
      },
      {
        "source": "/wizard-streamlit/**",
        "destination": "/wizard-streamlit/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}

